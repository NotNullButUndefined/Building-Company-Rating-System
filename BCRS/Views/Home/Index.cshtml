                            <div class="price-tooltip-title">Price</div>
                            <script type="text/javascript">
                                (function ($) {
                                    $(window).load(function () {
                                        var javo_PriceSlider_option = {
                                            start: [0, 30000000]
											, step: 1
											, range: { min: [0], max: [30000000] }
											, connect: true
											, serialization: {
											    lower: [
													$.Link({
													    target: $("input[name='minPrice']")
														, format: { decimals: 0 }
													}), $.Link({
													    target: '-tooltip-<div class="javo-slider-tooltip hidden-xs hidden-sm"></div>'
														, method: function (v) {
														    $(this).html('<span>' + v + '</span>');
														}, format: { decimals: 0, thousand: ',', prefix: '$' }
													})
											    ], upper: [
													$.Link({
